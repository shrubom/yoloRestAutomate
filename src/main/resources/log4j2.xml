<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="INFO">
	<Appenders>
		<Console name="STDOUT" target="SYSTEM_OUT">
			<PatternLayout pattern="[%-p] %m%n" />
		</Console>
	</Appenders>
	<Loggers>
		<Logger name="YoloApi" level="INFO" />		
		<Root level="trace">
			<AppenderRef ref="STDOUT" level="INFO" />
		</Root>
	</Loggers>
</Configuration>